n=int(input())

for l in range(0,n):
    sum=0
    k=int(input("enter the no"))
    for i in range(1,k+1):
        if(k%i==0):

            if(i//10==0):
                sum=sum+i
            if(i//10>0):
                for j in str(i):
                    sum=sum+int(j)



    if(k%sum==0):
        print("YES")
    else:
        print("NO")
    print(sum)
